@model IEnumerable<WebApplication1.Models.Booking>
    @{
        ViewData["Title"] = "Booking";
    }


    <h1>Bookings</h1>
    <table class="table table-striped table-hover table-borderless ">
        <tr>
            <th>Current charge </th>
            <th>distance</th>
            <th>Begin</th>
            <th>End</th>
        </tr>

        @foreach (var b in Model)
        {
            <tr>
                <td>@b.current_Charge</td>
                <td>@b.required_Distance</td>
                <td>@b.start_Time</td>
                <td>@b.end_Time</td>
            </tr>
        }
    </table>
