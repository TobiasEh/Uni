
@{ Booking[] bookings = Model;}

<h1>Buchungen</h1>
<table class="table table-hover table-striped">
    <tr>
        <th>Charge Status</th>
        <th>Distance</th>
        <th>Start Time</th>
        <th>End Time</th>
    </tr>

    @foreach (Booking item in Model)
    {
<tr>
    <td>
        @item.chargeStatus
        
    </td>
    <td>
        @item.distance
    </td>
    <td>
        @item.startTime
    </td>
    <td>
        @item.endTime
    </td>
</tr>
    }
</table>